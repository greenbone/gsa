{
  "presets": ["react-app"],
  "plugins": [
    [
      "i18next-extract",
      {
        "locales": ["de", "zh_TW", "zh_CN"],
        "pluralSeparator": null,
        "contextSeparator": null,
        "nsSeparator": null,
        "keySeparator": null,
        "outputPath": "public/locales/gsa-{{locale}}.json",
        "tFunctionNames": ["_", "_l"],
        "discardOldKeys": true
      }
    ]
  ]
}
