                       alt="{gsa:i18n ('Delete', 'Table Row')}"
                       title="{gsa:i18n ('Port List', 'Port List')}{gsa:i18n (' is still in use', 'Table Row')}"
                       style="margin-left:3px;"/>
                </xsl:otherwise>
              </xsl:choose>
            </td>
          </tr>
        </xsl:for-each>
      </table>
    </div>
  </div>
</xsl:template>

