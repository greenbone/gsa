                  <xsl:value-of select="concat (date:month-abbreviation (start/text()), ' ', date:day-in-month (start/text()), ' ', date:year (start/text()))"/>
                </a>
              </xsl:when>
              <xsl:otherwise>(<xsl:value-of select="gsa:i18n ('not finished', 'Host Window')"/>)</xsl:otherwise>
