            <xsl:when test="max_nvt_count='-1'">
            </xsl:when>
            <xsl:otherwise>
              <xsl:value-of select="gsa:i18n (' of ', 'Scan Config Table Row')"/>
              <xsl:value-of select="max_nvt_count"/>
