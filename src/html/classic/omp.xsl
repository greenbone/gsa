        <xsl:when test="//group[@name='method:ldap_connect']/auth_conf_setting[@key='enable']/@value = 'true'">
          <column>
            <name>Name</name>
          </column>
          <column>
            <name>Roles</name>
          </column>
          <column>
            <name>Groups</name>
          </column>
          <column>
            <name>Host Access</name>
          </column>
          <column>
            <name>LDAP Authentication</name>
            <field>ldap</field>
          </column>
        </xsl:when>
        <xsl:otherwise>
          <column>
            <name>Name</name>
          </column>
          <column>
            <name>Roles</name>
          </column>
          <column>
            <name>Groups</name>
          </column>
          <column>
            <name>Host Access</name>
          </column>
        </xsl:otherwise>
      </xsl:choose>
    </xsl:with-param>
    <xsl:with-param name="icon-count" select="4"/>
  </xsl:call-template>
</xsl:template>

