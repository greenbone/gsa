    <xsl:otherwise>
      <a href="/omp?cmd=get_report_section&amp;report_section={$section}&amp;report_id={@id}&amp;filter=&amp;filt_id=&amp;token={/envelope/token}"
        title="{gsa:i18n ('Return to default filter view', 'Window')}">
