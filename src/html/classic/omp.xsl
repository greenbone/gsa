            </xsl:choose>
          </td>
        </tr>
        <tr>
          <td><xsl:value-of select="gsa:i18n ('Event', 'Alert Window')"/>:</td>
          <td>
            <xsl:value-of select="gsa:i18n (event/text(), 'Alert Window')"/>
